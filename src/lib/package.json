{
  "name": "@qawaz/anique",
  "version": "1.0.4",
  "description": "A Minimal React & Emotion UI Component Library",
  "author": "Qawaz",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "homepage": "https://qawaz.github.io/anique",
  "files": [
    "dist/",
    "!dist/ts/**/*.tsbuildinfo"
  ],
  "repository": {
    "url": "https://github.com/Qawaz/anique.git",
    "type": "git",
    "directory": "src/lib"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "sideEffects": false,
  "keywords": [
    "ui",
    "ui-kit",
    "react",
    "next",
    "nextjs",
    "components",
    "component library",
    "library",
    "frontend",
    "design"
  ],
  "scripts": {
    "build": "yarn clean && yarn build:types && yarn build:js",
    "build:js": "rollup -c",
    "build:types": "tsc --declarationDir dist/types --declaration --emitDeclarationOnly",
    "clean": "rimraf dist .rollup.cache",
    "clean:all": "rimraf dist node_modules .rollup.cache",
    "test": "jest",
    "test:w": "jest --watch",
    "test:wa": "jest --watchAll",
    "test:c": "jest --coverage",
    "test:cv": "clear && jest --coverage --verbose",
    "ts-check": "tsc --noemit"
  },
  "dependencies": {

  },
  "devDependencies": {
    "@types/react": ">=18.2.15",
    "@types/react-dom": ">=18.2.7",
    "rimraf": "^5.0.1"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "@emotion/react": ">=11.9.0",
    "@emotion/styled": ">=11.8.1"
  }
}
